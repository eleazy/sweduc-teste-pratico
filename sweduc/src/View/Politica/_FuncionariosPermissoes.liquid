            <div class="switch-toggle-panel">
                <h3 class="h4">
                    Geral
                </h3>

                <div class="swtich-toggle-group">
                    <label class="switch-toggle" for="sistema-upload-acessar">
                        <input id="sistema-upload-acessar" type="checkbox" name="sistema-upload-acessar" {% if permissoes contains "sistema-upload-acessar" %}checked{% endif %}>
                        Acessar uploads
                    </label>

                    <label class="switch-toggle" for="sistema-backups-executar-backups">
                        <input id="sistema-backups-executar-backups" type="checkbox" name="sistema-backups-executar-backups" {% if permissoes contains "sistema-backups-executar-backups" %}checked{% endif %}>
                        Executar backups
                    </label>

                    <label class="switch-toggle" for="sistema-log-de-atividades-visualizar-logs">
                        <input id="sistema-log-de-atividades-visualizar-logs" type="checkbox" name="sistema-log-de-atividades-visualizar-logs" {% if permissoes contains "sistema-log-de-atividades-visualizar-logs" %}checked{% endif %}>
                        Visualizar logs de atividades
                    </label>
                </div>
            </div>
        </section>
    </div>

    <div style="margin: 1rem 0;">
        <button
            type="submit"
            class="btn primary-color"
            form="politicas-funcionarios"
        >
            Salvar
        </button>

        <button
            type="button"
            class="btn danger-color"
            onclick="sweduc.carregarUrl('config/politicas/funcionarios');"
        >
            Voltar
        </button>
    </div>
</form>
{% endblock %}
